{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "excludedTypes": "Request;Exception",
        "isEnabled": true
      }
    },
    "logLevel": {
      "Kerbee": "Information"
    }
  },
  "extensions": {
    "durableTask": {
      "extendedSessionsEnabled": true,
      "extendedSessionIdleTimeoutInSeconds": 120
    },
    "http": {
      "routePrefix": "",
      "customHeaders": {
        "Cache-Control": "no-store"
      }
    }
  }
}
